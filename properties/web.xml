<?xml version="1.0" encoding="ISO-8859-1"?>
  <web-app xmlns="http://java.sun.com/xml/ns/javaee" 
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"     
	 version="3.0">

	<!-- ============ Struts Support for WFP Applications ================= -->
	<servlet>
		<servlet-name>WFPApplicationsActionServlet</servlet-name>
		<servlet-class>com.kronos.wfc.platform.struts.framework.WFPApplicationsActionServlet</servlet-class>
		<init-param>
			<param-name>config/applications/cms</param-name>
			<param-value>/applications/cms/properties/struts-module.xml</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>

  </web-app>